syntax = "proto2";

package vinabatch;

message Ligand {
    required string name = 1;
    required string path = 2;
    // optional string email = 3;

}

message Job {
    repeated Ligand ligands = 1;
}

message modeSetting {
    required bool stop = 1;
}

